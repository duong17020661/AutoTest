<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Demo Web App test suite" parallel="tests"
	thread-count="1">
	<listeners>
		<listener
			class-name="reusableComponents.ListenersImplementation" />
		<listener
			class-name="reusableComponents.TestRetryAnalyzerListener" />
	</listeners>


	<test name="Create user">
		<classes>
			<class name="Tests.TestCase" />
		</classes>
	</test> <!-- Test -->
	<test name="Failed Test">
		<classes>
			<class name="Tests.Runner" />
		</classes>
	</test> <!-- Test -->

</suite> <!-- Suite -->
